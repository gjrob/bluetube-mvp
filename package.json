{
  "name": "bluetubetv",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' next build",
    "start": "next start",
    "smoke": "node scripts/smoke.js",
    "analytics:realtime": "node scripts/analytics-realtime.js",
    "lint": "next lint",
    "// Deployment Commands": "",
    "deploy": "npm run build && vercel --prod",
    "deploy:preview": "vercel",
    "logs": "vercel logs --follow",
    "// Analytics Commands": "",
    "analytics:revenue": "node scripts/check-revenue.js",
    "analytics:weekly": "node scripts/weekly-report.js",
    "// Blockchain Commands": "",
    "blockchain:balance": "node scripts/check-contract.js",
    "blockchain:withdraw": "node scripts/withdraw-fees.js",
    "blockchain:test": "node scripts/test-superchat.js",
    "// Database Commands": "",
    "db:migrate": "npx supabase db push",
    "db:seed": "node scripts/seed-database.js",
    "db:reset": "npx supabase db reset",
    "// Stripe Commands": "",
    "stripe:listen": "stripe listen --forward-to localhost:3000/api/webhooks/stripe",
    "stripe:test": "stripe trigger payment_intent.succeeded",
    "stripe:balance": "stripe balance",
    "// Testing Commands": "",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:payments": "node scripts/test-payments.js",
    "// Utility Commands": "",
    "clean": "rm -rf .next node_modules",
    "fresh": "npm run clean && npm install && npm run dev",
    "check": "npm run lint && npm run build"
  },
  "dependencies": {
    "@rainbow-me/rainbowkit": "^2.2.8",
    "@stripe/stripe-js": "^7.8.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/supabase-js": "^2.38.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.6.0",
    "buffer": "^6.0.3",
    "chalk": "^5.6.0",
    "dotenv": "^17.2.1",
    "ethers": "^5.7.2",
    "formidable": "^3.5.4",
    "hls.js": "^1.6.9",
    "jsonwebtoken": "^9.0.2",
    "jszip": "^3.10.1",
    "lucide-react": "^0.539.0",
    "micro": "^10.0.1",
    "multer": "^2.0.2",
    "next": "^13.4.19",
    "next-connect": "^1.0.0",
    "openai": "^5.12.2",
    "process": "^0.11.10",
    "puppeteer": "^24.16.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.5.2",
    "recharts": "^2.9.0",
    "sharp": "^0.34.3",
    "stripe": "^14.0.0",
    "uuid": "^11.1.0",
    "viem": "^2.34.0",
    "wagmi": "^2.16.4"
  },
  "engines": {
    "node": "22.x"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.0",
    "hardhat": "^2.26.3",
    "postcss": "^8",
    "tailwindcss": "^3.3.0"
  }
}
